{
	"name": "whatsnear",
	"version": "0.0.1",
	"private": true,
	"description": "An Ionic project",
	"author": "Matthias Fuchs",
	"scripts": {
		"start": "npm run start:dev",
		"start:dev": "ionic serve --no-open",
		"start:prod": "HOST_NAME='${HOST_NAME:=localhost}' PORT='${PORT:=8100}' ionic serve --no-open --consolelogs --external --no-livereload --prod --port=$PORT --host=0.0.0.0 --public-host=$HOST_NAME",
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"test:unit": "vue-cli-service test:unit",
		"test:e2e": "vue-cli-service test:e2e",
		"lint": "vue-cli-service lint",
		"eslint:check": "eslint \"{src,test}/**/*.ts\"",
		"eslint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
		"heroku:login": "heroku login && heroku container:login",
		"docker:build": "docker build --compress -t registry.heroku.com/whatsnear-frontend/web:latest .",
		"heroku:container:push": "heroku container:push web --app whatsnear-frontend",
		"heroku:container:pull": "heroku container:pull web --app whatsnear-frontend && heroku container:pull web --app whatsnear-backend",
		"heroku:container:release": "heroku container:release web --app whatsnear-frontend",
		"heroku:logs": "heroku logs --tail --app whatsnear-frontend",
		"docker:run:backend": "cd ../infrastructure && docker-compose -f docker-compose.yml up",
		"docker:run:frontend": "cd ../infrastructure && docker-compose -f docker-compose.frontend.yml up"
	},
	"dependencies": {
		"@capacitor/core": "2.4.5",
		"@capacitor/ios": "^2.4.5",
		"@ionic-native/globalization": "^5.31.1",
		"@ionic/vue": "^5.4.0",
		"@ionic/vue-router": "^5.4.0",
		"core-js": "^3.8.3",
		"geolib": "^3.3.1",
		"ionicons": "^5.4.0",
		"vue": "^3.0.0-0",
		"vue-class-component": "^8.0.0-0",
		"vue-router": "^4.0.2",
		"vue3-google-map": "^0.5.2"
	},
	"devDependencies": {
		"@capacitor/cli": "2.4.5",
		"@types/googlemaps": "^3.43.2",
		"@types/jest": "^24.0.19",
		"@types/node": "^12.19.15",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"@vue/cli-plugin-babel": "^4.5.11",
		"@vue/cli-plugin-eslint": "^4.5.11",
		"@vue/cli-plugin-router": "^4.5.11",
		"@vue/cli-plugin-typescript": "^4.5.11",
		"@vue/cli-plugin-unit-jest": "^4.5.11",
		"@vue/cli-service": "^4.5.11",
		"@vue/compiler-sfc": "^3.0.0-0",
		"@vue/eslint-config-standard": "^5.1.2",
		"@vue/eslint-config-typescript": "^7.0.0",
		"@vue/test-utils": "^2.0.0-rc.0",
		"babel-jest": "^26.6.3",
		"dotenv-webpack": "^6.0.0",
		"eslint": "^7.18.0",
		"eslint-config-prettier": "^7.2.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^3.3.1",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-standard": "^4.0.0",
		"eslint-plugin-vue": "^7.5.0",
		"eslint-webpack-plugin": "^2.4.3",
		"googlemaps": "^1.12.0",
		"prettier": "^2.2.1",
		"ts-node": "~8.3.0",
		"tslib": "^2.0.0",
		"typescript": "~3.9.3",
		"vue-jest": "^5.0.0-alpha.8",
		"vue-template-compiler": "^2.6.12"
	},
	"homepage": "https://whatsnear-frontend.herokuapp.com/"
}
